<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

    <bean class="jetbrains.buildServer.controllers.vcs.GitHubWebHookListener" init-method="register"/>
    <bean class="org.jetbrains.teamcity.github.controllers.CreateWebHookController" init-method="register"/>

    <bean class="org.jetbrains.teamcity.github.WebHooksManager" init-method="init" destroy-method="destroy">
        <constructor-arg index="2" ref="repositoryStateEventDispatcher"/>
    </bean>
    <bean class="org.jetbrains.teamcity.github.TokensHelper"/>

    <bean class="org.jetbrains.teamcity.github.GitHubWebHookAvailablePageExtension" init-method="register"/>
    <bean class="org.jetbrains.teamcity.github.GitHubWebHookAvailableHealthReport"/>
</beans>
